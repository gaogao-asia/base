{
  "name": "laravel-rocket/base",
  "description": "Base App Code For Laravel Rocket",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "aws/aws-sdk-php": "~3.24",
    "facebook/graph-sdk": "~5.6",
    "fideloper/proxy": "^4.0",
    "laravel-rocket/foundation": "2.5.*",
    "laravel-rocket/generator": "2.5.*",
    "laravel-rocket/service-auth": "2.5.*",
    "laravel/framework": "5.7.*",
    "laravel/passport": "^7.0",
    "laravel/socialite": "~3.0",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "5.7.*",
    "maatwebsite/excel": "^3.1",
    "mpdf/mpdf": "^7.1",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0",
    "symfony/thanks": "^1.0",
    "barryvdh/laravel-debugbar": "~3.0",
    "barryvdh/laravel-ide-helper": "~2.4",
    "doctrine/dbal": "~2.5",
    "friendsofphp/php-cs-fixer": "~2.8",
    "laravel/browser-kit-testing": "~2.0",
    "takaaki-mizuno/mwb-parser": "0.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
